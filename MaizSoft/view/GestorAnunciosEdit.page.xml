<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
      no-conversation-view-id="/GestorAnunciosList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{gestorAnunciosHome.wire}"/>

   <param name="gestorAnunciosFrom"/>
   <param name="gestorAnunciosIdGestorAnuncios" value="#{gestorAnunciosHome.gestorAnunciosIdGestorAnuncios}"/>
   <param name="actividadFrom"/>
   <param name="actividadIdActividad" value="#{actividadHome.actividadIdActividad}"/>
   <param name="tipoFrom"/>
   <param name="tipoIdTipo" value="#{tipoHome.tipoIdTipo}"/>




   <navigation from-action="#{gestorAnunciosHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/GestorAnuncios.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{gestorAnunciosHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/GestorAnuncios.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{gestorAnunciosHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/GestorAnunciosList.xhtml"/>
      </rule>
   </navigation>

</page>
