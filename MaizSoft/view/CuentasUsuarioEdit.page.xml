<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
      no-conversation-view-id="/CuentasUsuarioList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{cuentasUsuarioHome.wire}"/>

   <param name="cuentasUsuarioFrom"/>
   <param name="cuentasUsuarioId" value="#{cuentasUsuarioHome.cuentasUsuarioId}"/>
   <param name="grupoUsuariosFrom"/>
   <param name="grupoUsuariosIdGrupoUsuarios" value="#{grupoUsuariosHome.grupoUsuariosIdGrupoUsuarios}"/>
   <param name="grupoCursoFrom"/>
   <param name="grupoCursoIdGrupo" value="#{grupoCursoHome.grupoCursoIdGrupo}"/>
   <param name="cursoFrom"/>
   <param name="cursoCodigo" value="#{cursoHome.cursoCodigo}"/>
   <param name="enteUniversitarioFrom"/>
   <param name="enteUniversitarioIdEnteUniversitario" value="#{enteUniversitarioHome.enteUniversitarioIdEnteUniversitario}"/>
   <param name="tipoFrom"/>
   <param name="tipoIdTipo" value="#{tipoHome.tipoIdTipo}"/>




   <param name="usuarioFrom"/>
   <param name="usuarioId" value="#{usuarioHome.usuarioId}"/>


   <param name="userAccountFrom"/>
   <param name="userAccountId" value="#{userAccountHome.userAccountId}"/>



   <navigation from-action="#{cuentasUsuarioHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/CuentasUsuario.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{cuentasUsuarioHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/CuentasUsuario.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{cuentasUsuarioHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/CuentasUsuarioList.xhtml"/>
      </rule>
   </navigation>

</page>
