<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
      no-conversation-view-id="/ReceptorMensajesList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{receptorMensajesHome.wire}"/>

   <param name="receptorMensajesFrom"/>
   <param name="receptorMensajesIdReceptormenajes" value="#{receptorMensajesHome.receptorMensajesIdReceptormenajes}"/>
   <param name="gestorMensajeriaFrom"/>
   <param name="gestorMensajeriaIdMensaje" value="#{gestorMensajeriaHome.gestorMensajeriaIdMensaje}"/>
   <param name="userAccountFrom"/>
   <param name="userAccountId" value="#{userAccountHome.userAccountId}"/>
   <param name="enteUniversitarioFrom"/>
   <param name="enteUniversitarioIdEnteUniversitario" value="#{enteUniversitarioHome.enteUniversitarioIdEnteUniversitario}"/>
   <param name="tipoFrom"/>
   <param name="tipoIdTipo" value="#{tipoHome.tipoIdTipo}"/>






   <navigation from-action="#{receptorMensajesHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/ReceptorMensajes.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{receptorMensajesHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/ReceptorMensajes.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{receptorMensajesHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/ReceptorMensajesList.xhtml"/>
      </rule>
   </navigation>

</page>
