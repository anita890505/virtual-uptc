<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
      no-conversation-view-id="/NotaActividadList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{notaActividadHome.wire}"/>

   <param name="notaActividadFrom"/>
   <param name="notaActividadIdNotaActividad" value="#{notaActividadHome.notaActividadIdNotaActividad}"/>
   <param name="actividadFrom"/>
   <param name="actividadIdActividad" value="#{actividadHome.actividadIdActividad}"/>
   <param name="tipoFrom"/>
   <param name="tipoIdTipo" value="#{tipoHome.tipoIdTipo}"/>


   <param name="gestorCargaArchivosFrom"/>
   <param name="gestorCargaArchivosIdGestorCargaArchivos" value="#{gestorCargaArchivosHome.gestorCargaArchivosIdGestorCargaArchivos}"/>

   <param name="gestorMensajeriaFrom"/>
   <param name="gestorMensajeriaIdMensaje" value="#{gestorMensajeriaHome.gestorMensajeriaIdMensaje}"/>
   <param name="userAccountFrom"/>
   <param name="userAccountId" value="#{userAccountHome.userAccountId}"/>
   <param name="enteUniversitarioFrom"/>
   <param name="enteUniversitarioIdEnteUniversitario" value="#{enteUniversitarioHome.enteUniversitarioIdEnteUniversitario}"/>



   <param name="grupoUsuariosFrom"/>
   <param name="grupoUsuariosIdGrupoUsuarios" value="#{grupoUsuariosHome.grupoUsuariosIdGrupoUsuarios}"/>
   <param name="grupoCursoFrom"/>
   <param name="grupoCursoIdGrupo" value="#{grupoCursoHome.grupoCursoIdGrupo}"/>
   <param name="cursoFrom"/>
   <param name="cursoCodigo" value="#{cursoHome.cursoCodigo}"/>





   <navigation from-action="#{notaActividadHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/NotaActividad.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{notaActividadHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/NotaActividad.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{notaActividadHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/NotaActividadList.xhtml"/>
      </rule>
   </navigation>

</page>
