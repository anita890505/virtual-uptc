<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
      no-conversation-view-id="/GestorMensajeriaList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{gestorMensajeriaHome.wire}"/>

   <param name="gestorMensajeriaFrom"/>
   <param name="gestorMensajeriaIdMensaje" value="#{gestorMensajeriaHome.gestorMensajeriaIdMensaje}"/>
   <param name="userAccountFrom"/>
   <param name="userAccountId" value="#{userAccountHome.userAccountId}"/>
   <param name="grupoCursoFrom"/>
   <param name="grupoCursoIdGrupo" value="#{grupoCursoHome.grupoCursoIdGrupo}"/>
   <param name="cursoFrom"/>
   <param name="cursoCodigo" value="#{cursoHome.cursoCodigo}"/>
   <param name="escuelaFrom"/>
   <param name="escuelaIdEscuela" value="#{escuelaHome.escuelaIdEscuela}"/>
   <param name="facultadFrom"/>
   <param name="facultadIdFacultad" value="#{facultadHome.facultadIdFacultad}"/>


   <param name="semestreFrom"/>
   <param name="semestreIdSemestre" value="#{semestreHome.semestreIdSemestre}"/>



   <param name="gestorAnunciosFrom"/>
   <param name="gestorAnunciosIdAnuncio" value="#{gestorAnunciosHome.gestorAnunciosIdAnuncio}"/>




   <navigation from-action="#{gestorMensajeriaHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/GestorMensajeria.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{gestorMensajeriaHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/GestorMensajeria.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{gestorMensajeriaHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/GestorMensajeriaList.xhtml"/>
      </rule>
   </navigation>

</page>
